<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://www.govpay.it/servizi/pa"
	targetNamespace="http://www.govpay.it/servizi/pa" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	name="GP" jaxb:version="2.0">
	<wsdl:types>
		<xsd:schema targetNamespace="http://www.govpay.it/servizi/pa">
			<xsd:element name="gpCaricaPagamentoInAttesa" type="tns:pagamento" />

			<xsd:element name="gpCaricaPagamentoInAttesaResponse"
				type="tns:esitoOperazione" />

			<xsd:element name="gpCancellaPagamentoInAttesa">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codApplicazione" type="xsd:string" />
						<xsd:element name="idPagamento" type="tns:idPagamento" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpCancellaPagamentoInAttesaResponse"
				type="tns:esitoOperazione" />

			<xsd:element name="gpGeneraIUV">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codApplicazione" type="xsd:string" />
						<xsd:element name="codEnte" type="xsd:string" />
						<xsd:element name="tipoIuv" type="tns:tipoIuv" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpGeneraIUVResponse" type="tns:esitoOperazione" />

			<xsd:element name="gpInviaRpt">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codPortale" type="xsd:string" />
						<xsd:element name="idPagamento" type="tns:idPagamento" maxOccurs="unbounded" />
						<xsd:element name="autenticazione" type="tns:autenticazione" minOccurs="0" />
						<xsd:element name="versante" type="tns:anagrafica" minOccurs="0" />
						<xsd:element name="callbackUrl" type="xsd:string" />
						<xsd:element name="ibanAddebito" type="xsd:string" minOccurs="0" />
						<xsd:element name="canale" type="tns:canale" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpInviaRptResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="url" type="xsd:string" minOccurs="0" />
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpGeneraRpt">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codPortale" type="xsd:string" />
						<xsd:element name="pagamento" type="tns:pagamento" maxOccurs="unbounded" />
						<xsd:element name="autenticazione" type="tns:autenticazione" minOccurs="0" />
						<xsd:element name="versante" type="tns:anagrafica" minOccurs="0" />
						<xsd:element name="callbackUrl" type="xsd:string" />
						<xsd:element name="ibanAddebito" type="xsd:string" minOccurs="0" />
						<xsd:element name="canale" type="tns:canale" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpGeneraRptResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="url" type="xsd:string" minOccurs="0" />
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpChiediStatoPagamento">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice>
							<xsd:element name="codApplicazione" type="xsd:string" />
							<xsd:element name="codPortale" type="xsd:string" />
						</xsd:choice>
						<xsd:element name="idPagamento" type="tns:idPagamento" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="versamento">
				<xsd:sequence>
					<xsd:element name="stato" type="tns:statoPagamento" />
					<xsd:element name="xmlRT" type="xsd:base64Binary" minOccurs="0" />
					<xsd:element name="pagamento" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="idPagamento" type="tns:idPagamento" />
								<xsd:element name="debitore" type="tns:anagrafica" />
								<xsd:element name="importoTotale" type="tns:importo" />
								<xsd:element name="dataScadenza" type="xsd:date" />
								<xsd:element name="causale" type="xsd:string" />
								<xsd:sequence>
									<xsd:element name="singoloPagamento" minOccurs="1" maxOccurs="5">
										<xsd:complexType>
											<xsd:complexContent>
												<xsd:extension base="tns:singoloPagamento">
													<xsd:sequence>
														<xsd:element name="importoPagato" type="tns:importo" />
														<xsd:element name="iur" type="xsd:string"
															minOccurs="0" />
														<xsd:element name="stato" type="tns:statoSingoloPagamento" />
													</xsd:sequence>
												</xsd:extension>
											</xsd:complexContent>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>


			<xsd:element name="gpChiediStatoPagamentoResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="versamento" type="tns:versamento" />
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpCercaVersamentiRequest">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="limit" type="xsd:integer" minOccurs="0" />
						<xsd:element name="offset" type="xsd:integer" minOccurs="0" />
						<xsd:element name="codApplicazione" type="xsd:string" minOccurs="0" />
						<xsd:element name="codPortale" type="xsd:string" minOccurs="0" />
						<xsd:element name="codEnte" type="xsd:string" minOccurs="0" />
						<xsd:element name="codUnivocoDebitore" type="xsd:string" minOccurs="0" />
						<xsd:element name="terminato" type="xsd:boolean" minOccurs="0" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>


			<xsd:element name="gpCercaVersamentiResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="versamento" type="tns:versamento" minOccurs="0" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpInviaRr">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codPortale" type="xsd:string" />
						<xsd:element name="idPagamento" type="tns:idPagamento" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="gpInviaRrResponse" type="tns:esitoOperazione" />
			
			<xsd:element name="gpSceltaWISP">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codPortale" type="xsd:string" />
						<xsd:choice>
							<xsd:element name="codDominio" type="xsd:string" />
							<xsd:element name="codEnte" type="xsd:string" />
						</xsd:choice>
						<xsd:element name="keyPA" type="xsd:string" />
						<xsd:element name="keyWISP" type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			
			<xsd:element name="gpSceltaWISPResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="effettuazioneScelta" type="tns:effettuazioneSceltaWISP" />
								<xsd:element name="canale" type="tns:canale" minOccurs="0" />
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpChiediListaPsp">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codPortale" type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			
			<xsd:element name="gpChiediListaPspResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="psp" minOccurs="0" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="codPsp" type="xsd:string" />
											<xsd:element name="ragioneSociale" type="xsd:string" minOccurs="0"/>
											<xsd:element name="urlInfo" type="xsd:string" minOccurs="0"/>
											<xsd:element name="storno" type="xsd:boolean" />
											<xsd:element name="bollo" type="xsd:boolean" />
											<xsd:element name="canale" maxOccurs="unbounded">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="codCanale" type="xsd:string" />
														<xsd:element name="tipoVersamento" type="tns:tipoVersamento" />
														<xsd:element name="modelloVersamento" type="tns:modelloVersamento" />
														<xsd:element name="disponibilita" type="xsd:string" minOccurs="0"/>
														<xsd:element name="descrizione" type="xsd:string" minOccurs="0"/>
														<xsd:element name="condizioni" type="xsd:string" minOccurs="0"/>
														<xsd:element name="urlInfo" type="xsd:string" minOccurs="0"/>
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpGeneraCodiciAvviso">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice>
							<xsd:element name="codApplicazione" type="xsd:string" />
							<xsd:element name="codPortale" type="xsd:string" />
						</xsd:choice>
						<xsd:element name="idPagamento" type="tns:idPagamento" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpGeneraCodiciAvvisoResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="qrcode" type="xsd:base64Binary" minOccurs="0" />
								<xsd:element name="barcode" type="xsd:base64Binary" minOccurs="0" />
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpChiediFlussoRendicontazione">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice>
							<xsd:element name="codApplicazione" type="xsd:string" />
							<xsd:element name="idFr" type="xsd:long" />
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpChiediFlussoRendicontazioneResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="Fr" type="tns:Fr" minOccurs="0" />
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpChiediListaFlussiRendicontazione">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:choice>
							<xsd:element name="codApplicazione" type="xsd:string" />
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="gpChiediListaFlussiRendicontazioneResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="fr" minOccurs="0" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="idFr" type="xsd:long" />
											<xsd:element name="dataOraFlusso" type="xsd:dateTime" />
											<xsd:element name="numeroPagamenti" type="xsd:long" minOccurs="0"/>
											<xsd:element name="importoTotale" type="tns:importo" minOccurs="0"/>
											<xsd:element name="iur" type="xsd:string" minOccurs="0"/>
											<xsd:element name="dataRegolamento" type="xsd:date" minOccurs="0"/>
											<xsd:element name="stato" type="xsd:string" />
											<xsd:element name="descrizioneStato" type="xsd:string" minOccurs="0"/>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="paInviaEsitoPagamento">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="stato" type="tns:statoPagamento" />
						<xsd:element name="xmlRT" type="xsd:base64Binary" minOccurs="0" />
						<xsd:element name="pagamento" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="codApplicazione" type="xsd:string" />
									<xsd:element name="idPagamento" type="tns:idPagamento" />
									<xsd:element name="debitore" type="tns:anagrafica" />
									<xsd:element name="importoTotale" type="tns:importo" />
									<xsd:element name="dataScadenza" type="xsd:date" />
									<xsd:element name="causale" type="xsd:string" />
									<xsd:sequence>
										<xsd:element name="singoloPagamento" minOccurs="1" maxOccurs="5">
											<xsd:complexType>
												<xsd:complexContent>
													<xsd:extension base="tns:singoloPagamento">
														<xsd:sequence>
															<xsd:element name="importoPagato" type="tns:importo" />
															<xsd:element name="iur" type="xsd:string" minOccurs="0" />
															<xsd:element name="stato" type="tns:statoSingoloPagamento" />
														</xsd:sequence>
													</xsd:extension>
												</xsd:complexContent>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="paVerificaPagamentoInAttesa">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="codApplicazione" type="xsd:string" />
						<xsd:element name="idPagamento" type="tns:idPagamento" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="paVerificaPagamentoInAttesaResponse">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:esitoOperazione">
							<xsd:sequence>
								<xsd:element name="debitore" type="tns:anagrafica" />
								<xsd:element name="importoTotale" type="tns:importo" />
								<xsd:element name="dataScadenza" type="xsd:date" />
								<xsd:element name="causale" type="xsd:string" />
								<xsd:sequence>
									<xsd:element name="singoloPagamento" type="tns:singoloPagamento" minOccurs="1" maxOccurs="5" />
								</xsd:sequence>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="esitoOperazione">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element name="codApplicazione" type="xsd:string" />
						<xsd:element name="codPortale" type="xsd:string" />
					</xsd:choice>
					<xsd:element name="codOperazione" type="xsd:string" />
					<xsd:element name="codEsito" type="tns:codEsito" />
					<xsd:choice>
						<xsd:element name="idPagamento" type="tns:idPagamento" maxOccurs="unbounded" />
						<xsd:element name="codEnte" type="xsd:string" />
					</xsd:choice>
					<xsd:element name="codErrore" type="tns:codErrore" minOccurs="0" />
					<xsd:element name="descrizioneErrore" type="xsd:string" minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="canale">
				<xsd:sequence>
					<xsd:element name="codPsp" type="xsd:string" />
					<xsd:choice>
						<xsd:element name="codCanale" type="xsd:string" />
						<xsd:element name="tipoVersamento" type="tns:tipoVersamento" />
					</xsd:choice>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="wispKeys">
				<xsd:sequence>
			  		<xsd:element name="idDominio" type="xsd:string" />
					<xsd:element name="keyPA" type="xsd:string" />
					<xsd:element name="keyWISP" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="idPagamento">
				<xsd:sequence>
					<xsd:element name="codEnte" type="xsd:string" />
					<xsd:element name="iuv" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="pagamento">
				<xsd:sequence>
					<xsd:element name="codApplicazione" type="xsd:string" />
					<xsd:element name="codEnte" type="xsd:string" />
					<xsd:choice>
						<xsd:element name="iuv" type="xsd:string" />
						<xsd:element name="tipoIuv" type="tns:tipoIuv" />
					</xsd:choice>
					<xsd:element name="debitore" type="tns:anagrafica" />
					<xsd:element name="importoTotale" type="tns:importo" />
					<xsd:element name="dataScadenza" type="xsd:date" />
					<xsd:element name="causale" type="xsd:string" />
					<xsd:sequence>
						<xsd:element name="singoloPagamento" type="tns:singoloPagamento" minOccurs="1" maxOccurs="5" />
					</xsd:sequence>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:simpleType name="codEsito">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="OK"></xsd:enumeration>
					<xsd:enumeration value="KO"></xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>
			
			<xsd:simpleType name="effettuazioneSceltaWISP">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="SI"></xsd:enumeration>
					<xsd:enumeration value="NO"></xsd:enumeration>
					<xsd:enumeration value="PO"></xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>

			<xsd:simpleType name="codErrore">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ERRORE_AUTENTICAZIONE"></xsd:enumeration>
					<xsd:enumeration value="ERRORE_AUTORIZZAZIONE"></xsd:enumeration>
					<xsd:enumeration value="ERRORE_SEMANTICA"></xsd:enumeration>
					<xsd:enumeration value="ERRORE_INTERNO"></xsd:enumeration>
					<xsd:enumeration value="PAGAMENTO_NON_TROVATO"></xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>

			<xsd:simpleType name="statoPagamento">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="IN_ATTESA"></xsd:enumeration>
					<xsd:enumeration value="IN_CORSO"></xsd:enumeration>
					<xsd:enumeration value="AUTORIZZATO_IMMEDIATO">
						<xsd:annotation>
							<xsd:documentation>RPT Inviata con successo con profilo Immedato
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="AUTORIZZATO_DIFFERITO">
						<xsd:annotation>
							<xsd:documentation>RPT Inviata con successo con profilo Differito
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="AUTORIZZATO_PSP">
						<xsd:annotation>
							<xsd:documentation>RPT Inviata con successo a fronte di una
								Attivazione
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="PAGAMENTO_ESEGUITO">
						<xsd:annotation>
							<xsd:documentation>RT acquisita con successo con esito
								PAGAMENTO_ESEGUITO
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="PAGAMENTO_NON_ESEGUITO">
						<xsd:annotation>
							<xsd:documentation>RT acquisita con successo con esito
								PAGAMENTO_NON_ESEGUITO
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="PAGAMENTO_PARZIALMENTE_ESEGUITO">
						<xsd:annotation>
							<xsd:documentation>RT acquisita con successo con esito
								PAGAMENTO_PARZIALMENTE_ESEGUITO
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="DECORRENZA_TERMINI">
						<xsd:annotation>
							<xsd:documentation>RT acquisita con successo con esito
								DECORRENZA_TERMINI
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="DECORRENZA_TERMINI_PARZIALE">
						<xsd:annotation>
							<xsd:documentation>RT acquisita con successo con esito
								DECORRENZA_TERMINI_PARZIALE
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="ANNULLATO">
						<xsd:annotation>
							<xsd:documentation>Pagamento annullato su richiesta
								dell'Applicazione
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="SCADUTO">
						<xsd:annotation>
							<xsd:documentation>Data di scadenza del pagamento decorsa
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
					<xsd:enumeration value="FALLITO">
						<xsd:annotation>
							<xsd:documentation>RPT inviata e terminata in stato terminale di
								KO
							</xsd:documentation>
						</xsd:annotation>
					</xsd:enumeration>
				</xsd:restriction>
			</xsd:simpleType>

			<xsd:simpleType name="statoSingoloPagamento">
