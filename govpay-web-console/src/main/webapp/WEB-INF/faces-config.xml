			<to-view-id>/pages/anagrafica/psp/list/listaPsp.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaEnti</from-outcome>
			<to-view-id>/pages/anagrafica/enti/list/listaEnti.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
