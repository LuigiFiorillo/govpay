			<xsd:element name="codEnte" type="xsd:string"/>
			<xsd:element name="idDominio" type="id-dominio">
				
			</xsd:element>
			<xsd:element name="abilitato" type="xsd:boolean"/>
			
			<xsd:element minOccurs="0" name="idTemplateRPT" type="id-mail-template">
				
			</xsd:element>
			
			<xsd:element minOccurs="0" name="idTemplateRT" type="id-mail-template">
				
			</xsd:element>
			
			<xsd:element name="invioMailRPTAbilitato" type="xsd:boolean"/>
			<xsd:element name="invioMailRTAbilitato" type="xsd:boolean"/>
			
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="Portale" type="Portale"/>
	<xsd:complexType name="Portale">
		
		<xsd:sequence>
			<xsd:element name="codPortale" type="xsd:string"/>
			<xsd:element name="defaultCallbackURL" type="String512"/>
			<xsd:element name="principal" type="xsd:string"/>
			<xsd:element name="abilitato" type="xsd:boolean"/>
			<xsd:element maxOccurs="unbounded" ref="PortaleApplicazione"/>
		</xsd:sequence>
