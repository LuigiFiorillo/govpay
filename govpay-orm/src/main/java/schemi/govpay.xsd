	<xsd:complexType name="TracciatoXML">
		
		<xsd:sequence>
			<xsd:element name="tipoTracciato" type="xsd:string"/>
			<xsd:element name="codMessaggio" type="xsd:string"/>
			<xsd:element name="dataOraCreazione" type="xsd:dateTime"/>
			<xsd:element name="xml" type="xsd:base64Binary"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Anagrafica -->

