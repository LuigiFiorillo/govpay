			<xsd:element name="mittente" type="xsd:string"/>
			<xsd:element name="templateOggetto" type="String512">
				
			</xsd:element>
			<xsd:element name="templateMessaggio" type="xsd:string">
				
			</xsd:element>
			<xsd:element minOccurs="0" name="allegati" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
