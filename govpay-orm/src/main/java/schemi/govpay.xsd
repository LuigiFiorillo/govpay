			<xsd:element name="codMsgRevoca" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-iban-accredito">
		<xsd:sequence>
			<xsd:element name="codIban" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="id-er">
