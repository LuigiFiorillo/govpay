		<xsd:sequence>
			<xsd:element name="idAnagraficaEnte" type="id-anagrafica">
				
			</xsd:element>
			<!--xsd:element name="idOperatore" type="id-operatore" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:appinfo>
						<linkit:generator xmlns:linkit="http://www.link.it/project_generator">
