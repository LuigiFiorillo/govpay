			<xsd:element minOccurs="0" name="indirizzo" type="xsd:string"/>
			<xsd:element minOccurs="0" name="civico" type="xsd:string"/>
			<xsd:element minOccurs="0" name="cap" type="xsd:string"/>
			<xsd:element minOccurs="0" name="localita" type="xsd:string"/>
			<xsd:element minOccurs="0" name="provincia" type="xsd:string"/>
			<xsd:element minOccurs="0" name="nazione" type="xsd:string"/>
			<xsd:element minOccurs="0" name="email" type="xsd:string"/>
			<xsd:element minOccurs="0" name="telefono" type="xsd:string"/>
			<xsd:element minOccurs="0" name="cellulare" type="xsd:string"/>
			<xsd:element minOccurs="0" name="fax" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="MailTemplate" type="MailTemplate"/>
