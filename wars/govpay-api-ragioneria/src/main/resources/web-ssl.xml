<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">

	<display-name>govpay-api-ragioneria</display-name>
    
    <listener>
	    <listener-class>it.govpay.ragioneria.api.listener.InitListener</listener-class>
	</listener>
	
	<filter>
		<filter-name>OriginFilter</filter-name>
		<filter-class>it.govpay.rs.OriginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>OriginFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

</web-app>
