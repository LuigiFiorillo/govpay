<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet-mapping>
		<servlet-name>javax.ws.rs.core.Application</servlet-name>
		<url-pattern>/rs/*</url-pattern>
	</servlet-mapping>
	
	<!-- PagamentiTelematiciGPRnd  -->
	<servlet id="GPRnd_v1">
		<servlet-name>GPRnd_v1</servlet-name>
		<servlet-class>it.govpay.web.ws.PagamentiTelematiciGPRndImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPRnd_v1</servlet-name>
		<url-pattern>/PagamentiTelematiciGPRndService</url-pattern>
	</servlet-mapping>
	<servlet id="GPRnd_v2">
		<servlet-name>GPRnd_v2</servlet-name>
		<servlet-class>it.govpay.web.ws.v2_3.PagamentiTelematiciGPRndImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPRnd_v2</servlet-name>
		<url-pattern>/PagamentiTelematiciGP/soap/2.3/Rendicontazioni</url-pattern>
	</servlet-mapping>
	<servlet id="GPRnd_v3">
		<servlet-name>GPRnd_v3</servlet-name>
		<servlet-class>it.govpay.web.ws.v2_5.PagamentiTelematiciGPRndImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPRnd_v3</servlet-name>
		<url-pattern>/PagamentiTelematiciGP/soap/v3/Rendicontazioni</url-pattern>
	</servlet-mapping>
	
	
	
	<!-- PagamentiTelematiciGPApp  -->
	<servlet id="GPApp_v1">
		<servlet-name>GPApp_v1</servlet-name>
		<servlet-class>it.govpay.web.ws.PagamentiTelematiciGPAppImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPApp_v1</servlet-name>
		<url-pattern>/PagamentiTelematiciGPAppService</url-pattern>
	</servlet-mapping>
	<servlet id="GPApp_v2">
		<servlet-name>GPApp_v2</servlet-name>
		<servlet-class>it.govpay.web.ws.v2_3.PagamentiTelematiciGPAppImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPApp_v2</servlet-name>
		<url-pattern>/PagamentiTelematiciGP/soap/2.3/Applicazioni</url-pattern>
	</servlet-mapping>
	<servlet id="GPApp_v3">
		<servlet-name>GPApp_v3</servlet-name>
		<servlet-class>it.govpay.web.ws.v2_5.PagamentiTelematiciGPAppImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPApp_v3</servlet-name>
		<url-pattern>/PagamentiTelematiciGP/soap/v3/Applicazioni</url-pattern>
	</servlet-mapping>
	
	
	
	<!-- PagamentiTelematiciGPPrt  -->
	
	
	<servlet id="GPPrt_v1">
		<servlet-name>GPPrt_v1</servlet-name>
		<servlet-class>it.govpay.web.ws.PagamentiTelematiciGPPrtImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPPrt_v1</servlet-name>
		<url-pattern>/PagamentiTelematiciGPPrtService</url-pattern>
	</servlet-mapping>
	
	<servlet id="GPPrt_v2">
		<servlet-name>GPPrt_v2</servlet-name>
		<servlet-class>it.govpay.web.ws.v2_3.PagamentiTelematiciGPPrtImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPPrt_v2</servlet-name>
		<url-pattern>/PagamentiTelematiciGP/soap/2.3/Portali</url-pattern>
	</servlet-mapping>
	
	<servlet id="GPPrt_v3">
		<servlet-name>GPPrt_v3</servlet-name>
		<servlet-class>it.govpay.web.ws.v2_5.PagamentiTelematiciGPPrtImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GPPrt_v3</servlet-name>
		<url-pattern>/PagamentiTelematiciGP/soap/v3/Portali</url-pattern>
	</servlet-mapping>
	
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>Protetto</web-resource-name>
			<url-pattern>/PagamentiTelematiciCCPservice</url-pattern>
			<url-pattern>/PagamentiTelematiciRTservice</url-pattern>
			<url-pattern>/PagamentiTelematiciGPAppService</url-pattern>
			<url-pattern>/PagamentiTelematiciGPPrtService</url-pattern>
			<url-pattern>/PagamentiTelematiciGPRndService</url-pattern>
			<url-pattern>/PagamentiTelematiciGP/soap/*</url-pattern>
			<url-pattern>/rs/caricatore/*</url-pattern>
			<url-pattern>/rs/v1/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>*</role-name>
		</auth-constraint>
	</security-constraint>

	<security-role>
		<role-name>*</role-name>
	</security-role>

	<login-config>
		<auth-method>BASIC</auth-method>
	</login-config>

</web-app>
